{
  "name": "Language Extention",
  "description": "Make Language Greate Again!",
  "version": "0.1",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "activeTab", "scripting"],
  "action": {
    "default_title": "Make this page red",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["map.js", "content.js"],
      "css": ["content.css"]
    }
  ]
}
